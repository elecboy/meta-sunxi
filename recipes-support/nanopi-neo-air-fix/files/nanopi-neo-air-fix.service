[Unit]
Description=mtsports nanopi-neo-air board func fix
After=network.target

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/sbin/iw wlan0 set power_save off
ExecStart=/bin/echo ds3231 0x68 > /sys/class/i2c-adapter/i2c-0/new_device
ExecStart=/sbin/hwclock -r

Restart=no

[Install]
WantedBy=multi-user.target